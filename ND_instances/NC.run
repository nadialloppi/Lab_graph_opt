param UB_LR;

param norma default 1;

let step := 1;

for {iter in 1..10} 
{
  
  # Solve the Lagrangian Relaxation
  solve LagrRelaxation;
  
  # Check for feasibility: 
  # update UB_LR if feasible
  param temp default 0;
  for {k in K} {
  	if (sum {(i, j) in A} x[i, j, k] > h) then {
  		let temp:=1;
  		break;
  }}
  if (temp=0) then let UB_LR:= Obj_LR;

  # Update step size
  let step:= 0.995*step;
 
  # Update LB_LR 
  let LB_LR := Obj_LR;

  # Update norma
  let norma:= ( sum{k in K} (sum {(i, j) in A} x[i, j, k] - h)^2) ^ (1/2);
	
	  
  # Update multipliers
  
  # Memorize old values
  for {k in K}
  	let mu_old[k] := mu[k];

  # Calculate new values
  for {k in K}
  	let mu[k]:= max{0, mu_old[k] + step*(sum {(i, j) in A} x[i, j, k] - h)/norm};
  	
         
}
